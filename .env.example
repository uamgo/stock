# 项目根目录的环境变量配置示例
# 复制此文件为 .env 并根据需要修改配置

# 数据库配置
MONGODB_URI=mongodb://localhost:27017/
DATABASE_NAME=stock_trading

# 日志配置
LOG_LEVEL=INFO

# API配置
API_TIMEOUT=30
API_RETRY_COUNT=3

# 缓存配置
CACHE_ENABLED=true
CACHE_EXPIRE_MINUTES=10

# 交易配置
DEFAULT_PRESET=balanced

# 输出配置
OUTPUT_DIR=/Users/kevin/Downloads
